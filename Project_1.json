{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 400,
  "title": {
    "text": [
      "Female Labour Force Participation in Chile",
      "and the United Kingdom (1990-2024)"
    ],
    "anchor": "start",
    "fontSize": 14,
    "fontWeight": "bold"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/AntoniaDahuabe/AntoniaDahuabe.github.io/refs/heads/main/Project_labour_force_NorthenEurope_SouthAmerica.csv"
  },
  "mark": {"type": "line", "strokeWidth": 5, "opacity": 0.7},
  "transform": [
    {"filter": "datum['sex.label'] === 'Sex: Female'"},
    {
      "filter": "(datum['ref_area.label'] === 'Chile' || datum['ref_area.label'] === 'United Kingdom of Great Britain and Northern Ireland')"
    }
  ],
  "encoding": {
    "x": {
      "field": "time",
      "type": "temporal",
      "timeUnit": "year",
      "axis": {"title": "Year"}
    },
    "y": {
      "field": "obs_value",
      "type": "quantitative",
      "title": "Labour Force Participation Rate",
      "scale": {"domain": [0, 70]}
    },
    "color": {
      "field": "[ref_area.label]",
      "type": "nominal",
      "title": null,
      "legend": {"orient": "top", "symbolSize": 2000},
      "scale": {"range": ["#B34C4E", "teal"]}
    },
    "tooltip": [
      {"field": "[ref_area.label]", "type": "nominal", "title": "Country"},
      {
        "field": "time",
        "type": "temporal",
        "title": "Year",
        "timeUnit": "year"
      },
      {
        "field": "obs_value",
        "type": "quantitative",
        "title": "Participation Rate"
      }
    ]
  },
  "config": {}
}