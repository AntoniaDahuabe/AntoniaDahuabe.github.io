{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Vietnam's Exports",
    "anchor": "start",
    "subtitle": "Current Billion USD | Source: World Bank through ECO API",
    "fontSize": 14,
    "subtitleFontSize": 12,
    "color": "#254397",
    "subtitleColor": "#000000"
  },
  "height": 250,
  "width": 250,
  "autosize": {"type": "fit", "contains": "padding"},
  "background": "#ffffff",
  "view": {"fill": "#ffffff", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/ukr/expo?vega",
    "format": {"type": "json"}
  },
  "mark": {"type": "line", "color": "#254397", "strokeWidth": 2},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    }
  },
  "config": {}
}